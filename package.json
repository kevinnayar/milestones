    "start:worker": "NODE_ENV=development nodemon --watch bin --watch server --watch shared -e ts --exec npx ts-node bin/startWorkers.ts",
    "reboot:db": "NODE_ENV=development ts-node bin/rebootDatabase.ts",
    "build": "tsc"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
